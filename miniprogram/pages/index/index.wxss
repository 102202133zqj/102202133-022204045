.container {
  padding: 20px;
}

.search-bar {
  display: flex;
  margin-bottom: 40px;
}

.search-input {
  flex: 1;
  padding: 10px;
  border: 1px solid #ddd;
  border-radius: 4px;
  font-size: 16px;
}

.search-button {
  padding: 10px 10px;
  margin-left: 10px;
  background-color: #07C160;
  color: #fff;
  border: none;
  border-radius: 4px;
  font-size: 16px;
}

.main {
  background-color: #fff;
  border-radius: 8px;
  padding: 20px;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
}

.title {
  font-size: 24px;
  font-weight: bold;
  margin-bottom: 20px;
}

.project-item {
  padding: 10px;
  border-bottom: 1px solid #eee;
}

.project-title {
  font-size: 18px;
  font-weight: bold;
}

.project-description {
  font-size: 16px;
  color: #666;
}