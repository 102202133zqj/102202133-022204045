.container {
  padding: 20px;
}

.main {
  background-color: #fff;
  border-radius: 8px;
  padding: 20px;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
}

.title {
  font-size: 24px;
  font-weight: bold;
  margin-bottom: 20px;
}

.section {
  margin-bottom: 20px;
}

.section-title {
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 10px;
}

.project-item {
  padding: 10px;
  border-bottom: 1px solid #eee;
}

.project-title {
  font-size: 18px;
  font-weight: bold;
}

.project-description {
  font-size: 16px;
  color: #666;
}